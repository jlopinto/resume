<!doctype html>
<html lang="fr">
    <head>
        {% block head %}
            <meta charset="utf-8"/>
            <meta content="ie=edge" http-equiv="x-ua-compatible"/>
            <title>{{ pageTitle | default("Curriculum vitæ – Intégrateur web / développeur frontend") }}</title>
            <meta name="description" content="{{ pageDescription | default("Site description") }}"/>
            <meta content="width=device-width, initial-scale=1" name="viewport"/>
            <link href="dist/apple-touch-icon.png" rel="apple-touch-icon"/>
            <link rel="icon" href="favicon.ico"/>
            <link rel="preload" href="https://fonts.googleapis.com/css?family=Livvic%3A300%2C400%2C500%2C600%2C700%7CRaleway%3A300%2C400%2C500%2C600%2C700%26display%3Dswap" as="style">
            <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Livvic%3A300%2C400%2C500%2C600%2C700%7CRaleway%3A300%2C400%2C500%2C600%2C700%26display%3Dswap">
            <link rel="preload" href="dist/main.css" as="style">
            <link rel="stylesheet" href="dist/main.css" media="all">
        {% endblock %}
    </head>
    <body>
        <div class="resume">
            <div style="display:none">
                {% include '../../../dist/images/svg-sprite.svg' %}
            </div>
            {% block mainHeader %}
                {% include 'main-header.twig' %}
            {% endblock %}
            {% block mainContent %}
                {% include 'main-content.twig' %}
            {% endblock %}
        </div>
        {% block scripts %}
            <script src="dist/main.js"></script>
        {% endblock %}
    </body>
</html>
